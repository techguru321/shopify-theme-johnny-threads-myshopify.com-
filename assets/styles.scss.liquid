.navbar-default{
  border-color:#C8C8C8;
  -webkit-transition: all 0.2s ease-in-out;
  -o-transition: all 0.2s ease-in-out;
  transition: all 0.2s ease-in-out;
  background:#111214;
  border-radius: 0px;
  margin-bottom:0px;
  padding-top: 5px;
  font-size: 12px;
  position:relative;
  .navbar-nav{
    li{
      &.open{
        a,a:hover, a:focus{
          color: #C8C8C8;
          background-color: rgba(0,0,0,0);
        }
      }
      a{        
        text-transform:uppercase;
        color: #C8C8C8;
        background-color: rgba(0,0,0,0);
        padding-left:0px;
        padding-right:0px;
        &:hover, &:focus{
          color: #FFF;
        }
      }
      &.active{
        a{
          color: #C8C8C8;
          background-color: rgba(0,0,0,0);
          &:hover, &:focus{
            background-color: rgba(0,0,0,0);
          }
        }
      }
    }
  }  
}


